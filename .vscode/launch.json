{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Executable",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/bin/coveralls.dart",
      "args": ["var/lcov.info"]
    },
    {
      "name": "Tests",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/test/",
      "enableAsserts": true
    }
  ]
}
